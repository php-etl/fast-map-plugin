{
    "name": "php-etl/fast-map-plugin",
    "description": "Adapters for the Fast Map mapping library",
    "type": "satellite-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.2",
        "php-etl/fast-map-config": "^0.3.0",
        "php-etl/configurator-contracts": "^0.3.0",
        "php-etl/packaging-contracts": "^0.1.0",
        "php-etl/satellite-toolbox": "^0.1.0",
        "php-etl/pipeline-contracts": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpunit/php-invoker": "*",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpspec/phpspec": "^7.0",
        "friends-of-phpspec/phpspec-code-coverage": "*",
        "adlawson/vfs": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\FastMap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\FastMap\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        },
        "satellite": {
            "class": "Kiboko\\Plugin\\FastMap\\Service"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
